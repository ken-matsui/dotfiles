Host *
  ServerAliveInterval 60

Host attu
  # works with any attu
  Hostname attu4.cs.washington.edu
  IdentityAgent "~/.1password/agent.sock"

Host kopis
  Hostname kopis.cs.washington.edu
  IdentityAgent "~/.1password/agent.sock"
  ProxyJump attu

Host thinkpad
  Hostname kmatsui-ThinkPad-X1-Carbon-Gen-9.local
  IdentityAgent "~/.1password/agent.sock"

Host gcc.gnu.org
  ForwardX11 no
  IdentityAgent "~/.1password/agent.sock"

Host *
  IdentityAgent "~/.1password/agent.sock"
