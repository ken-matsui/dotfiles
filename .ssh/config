Host attu
  # works with any attu
  Hostname attu.cs.washington.edu
  IdentityAgent "~/.1password/agent.sock"

Host kopis
  Hostname kopis.cs.washington.edu
  IdentityAgent "~/.1password/agent.sock"
  ProxyJump attu

Host gcc.gnu.org
  ForwardX11 no
  IdentityAgent "~/.1password/agent.sock"

Host *
  IdentityAgent "~/.1password/agent.sock"
